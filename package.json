{
  "name": "spyne",
  "sideEffects": false,
  "version": "0.14.8",
  "description": "Reactive Real-DOM Framework for Advanced Javascript applications",
  "main": "lib/spyne.min.js",
  "module": "src/spyne/spyne.js",
  "scripts": {
    "eslint-fix-dry": "eslint 'src/spyne/**' --fix-dry-run",
    "eslint-fix": "eslint --fix  'src/spyne/**'",
    "eslint-fix-tests": "eslint --fix  'src/tests/**'",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commit": "git-cz",
    "build": "webpack --mode production   --env build --display-modules  ",
    "start": "webpack-dev-server --hot ",
    "dev": "webpack --mode development --progress --colors --watch --env dev ",
    "stats": "webpack --profile --json --optimize-minimize  --mode production --env build  > stats.json",
    "docs": "jsdoc --readme README.md -d docs  -c docs/jsdoc.config.json -r",
    "esdocs": "esdoc -d esdocs  -c esdocs/esdoc.config.json -r",
    "test": "karma start --browsers Chrome",
    "test:single": "npm test -- --single-run",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "test:mocha": "mocha tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spynejs/spyne.git"
  },
  "keywords": [],
  "author": "Frank Batista",
  "license": "LGPL",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "classlist-polyfill": "^1.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "es6-promise": "^4.2.8",
    "eslint": "^6.1.0",
    "eslint-config-semistandard": "^14.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^24.9.0",
    "file-loader": "^4.2.0",
    "fstream": "^1.0.12",
    "ghooks": "^2.0.4",
    "history": "^4.9.0",
    "immutable": "^3.8.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "json-loader": "^0.5.7",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine-es6": "0.0.3",
    "karma-mocha": "^1.3.0",
    "karma-virtualbox-ie11-launcher": "^1.2.2",
    "karma-webpack": "^4.0.2",
    "marked": "^0.7.0",
    "mocha": "^6.2.0",
    "mocha-loader": "^3.0.0",
    "mocha-webpack": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "opt-cli": "^1.6.0",
    "predicado": "0.0.6",
    "preload-image-loader": "^1.0.0",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "semistandard": "^13.0.1",
    "tar": "^4.4.10",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-rxjs-externals": "^2.0.0",
    "webpack-shell-plugin": "^0.5.0",
    "yargs": "13.3.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "data.validation": "^1.3.1",
    "ramda": "^0.27.0",
    "rxjs": "^6.5.2",
    "spynejs-polyfill": "0.0.4"
  }
}
