{
  "name": "spyne",
  "sideEffects": false,
  "version": "0.16.0",
  "description": "Reactive Real-DOM Framework for Advanced Javascript applications",
  "main": "lib/spyne.min.js",
  "module": "src/spyne/spyne.js",
  "scripts": {
    "eslint-fix-dry": "eslint 'src/spyne/**' --fix-dry-run",
    "eslint-fix": "eslint --fix  'src/spyne/**'",
    "eslint-fix-tests": "eslint --fix  'src/tests/**'",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commit": "git-cz",
    "build": "webpack --mode production   --env build ",
    "start": "webpack-dev-server --hot ",
    "dev": "webpack --mode development --progress  --watch --env dev ",
    "stats": "webpack --profile --json --optimize-minimize  --mode production --env build  > stats.json",
    "docs": "jsdoc --readme README.md -d docs  -c docs/jsdoc.config.json -r",
    "esdocs": "esdoc -d esdocs  -c esdocs/esdoc.config.json -r",
    "test": "karma start --browsers Chrome",
    "test:single": "npm test -- --single-run",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "test:mocha": "mocha tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spynejs/spyne.git"
  },
  "keywords": [],
  "author": "Frank Batista",
  "license": "LGPL",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chai-dom": "^1.9.0",
    "classlist-polyfill": "^1.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "css-loader": "^5.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "es6-promise": "^4.2.8",
    "eslint": "^7.22.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "expect": "^26.6.2",
    "file-loader": "^6.2.0",
    "fstream": "^1.0.12",
    "ghooks": "^2.0.4",
    "history": "^5.0.0",
    "immutable": "^3.8.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.6",
    "json-loader": "^0.5.7",
    "karma": "^6.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine-es6": "0.0.3",
    "karma-mocha": "^2.0.1",
    "karma-virtualbox-ie11-launcher": "^1.2.7",
    "karma-webpack": "^5.0.0",
    "marked": "^2.0.1",
    "mocha": "^8.3.2",
    "mocha-loader": "^5.1.5",
    "mocha-webpack": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "opt-cli": "^1.6.0",
    "preload-image-loader": "^1.0.0",
    "resolve-url-loader": "^3.1.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-rxjs-externals": "^2.0.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "data.validation": "^1.3.1",
    "ramda": "^0.27.1",
    "rxjs": "^6.6.6",
    "spynejs-polyfill": "0.0.5"
  }
}
